{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/components": [
        "src/components/*"
      ],
      "@/assets": [
        "src/assets/*"
      ],
      "@/lib": [
        "src/lib/*"
      ],
      "@/utils": [
        "src/utils/*"
      ],
      "@/hooks": [
        "src/hooks/*"
      ],
      "@/storage": [
        "src/storage/*"
      ],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "extends": "astro/tsconfigs/strict"
}